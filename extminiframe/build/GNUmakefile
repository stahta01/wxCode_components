	$(CXX) -o $@ $(MINIMAL_OBJECTS)  $(VAR_2) -L../lib  $(__minimal_RPATH) $(LDFLAGS)  ../lib/gcc_$(__extminiframe_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_extminiframe-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs core,base`

make_sample_dir_minimal: 
	@mkdir -p ../sample

tarball: 
	make distclean
	( cd ../.. && tar -cvzf extminiframe.tar.gz --exclude="*~" --exclude="*.log" --exclude="*.o*" --exclude="*.a" --exclude=".svn" --exclude="autom4te.cache" extminiframe/* )

zip: clean
	del ../../extminiframe.zip
	( cd ../.. && zip -r9 extminiframe.zip extminiframe -x "*.pdb" -x "*.log" -x "*.o*" )

