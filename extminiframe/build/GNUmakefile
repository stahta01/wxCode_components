ifeq ($(WX_SHARED),0)
../lib/gcc_$(__extminiframe_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_extminiframe-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_extminiframe_lib  $(EXTMINIFRAME_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(EXTMINIFRAME_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(WX_SHARED),0)
install_extminiframe_lib: $(__extminiframe_lib___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__extminiframe_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_extminiframe-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
