endif

make_dir_extminiframe_lib: 
	@mkdir -p ../lib/gcc_$(__extminiframe_lib__DIRNAME_SHARED_SUFFIX)

ifeq ($(WX_SHARED),0)
install_extminiframe_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(EXTMINIFRAME_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),0)
uninstall_extminiframe_lib_headers: 
	for f in $(EXTMINIFRAME_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),1)
../lib/gcc_$(__extminiframe_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_extminiframe-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_extminiframe_dll  $(EXTMINIFRAME_DLL_OBJECTS)
	$(CXX) $(LINK_DLL_FLAGS) -fPIC -o $@ $(EXTMINIFRAME_DLL_OBJECTS)  $(VAR_2) -L../lib $(LDFLAGS)  `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs core,base`
endif

ifeq ($(WX_SHARED),1)
install_extminiframe_dll: $(__extminiframe_dll___depname)
