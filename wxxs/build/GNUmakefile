make_dir_wxxs_lib: 
	@mkdir -p ../lib/gcc_$(__wxxs_lib__DIRNAME_SHARED_SUFFIX)

ifeq ($(WX_SHARED),0)
install_wxxs_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in ; do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),0)
uninstall_wxxs_lib_headers: 
	for f in ; do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),1)
../lib/gcc_$(__wxxs_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxxs-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_wxxs_dll  $(WXXS_DLL_OBJECTS)
