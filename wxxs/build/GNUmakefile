../lib/gcc_$(__wxxs_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxxs-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_wxxs_lib  $(WXXS_LIB_OBJECTS)
