all: gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)
gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX):
	@mkdir -p gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)

### Targets: ###

all: test_for_selected_wxbuild $(__rarinputstream_lib___depname) $(__rarinputstream_dll___depname) ../sample/minimal

install: $(__install_rarinputstream_lib___depname) $(__install_rarinputstream_lib_headers___depname) $(__install_rarinputstream_dll___depname) $(__install_rarinputstream_dll_headers___depname)

uninstall: $(__uninstall_rarinputstream_lib___depname) $(__uninstall_rarinputstream_lib_headers___depname) $(__uninstall_rarinputstream_dll___depname) $(__uninstall_rarinputstream_dll_headers___depname)

clean: 
	rm -f gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)/*.o
	rm -f gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)/*.d
	rm -f ../lib/gcc_$(__rarinputstream_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_rarinputstream-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
	rm -f ../lib/gcc_$(__rarinputstream_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_rarinputstream-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f ../lib/gcc_$(__rarinputstream_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_rarinputstream-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f ../sample/minimal

