	@$(WX_CONFIG) $(WX_CONFIG_FLAGS)

ifeq ($(SHARED),0)
../lib/gcc_$(__rarinputstream_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_rarinputstream-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_rarinputstream_lib  $(RARINPUTSTREAM_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(RARINPUTSTREAM_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(SHARED),0)
install_rarinputstream_lib: $(__rarinputstream_lib___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__rarinputstream_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_rarinputstream-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
endif

ifeq ($(SHARED),0)
uninstall_rarinputstream_lib: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_rarinputstream-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_rarinputstream_lib: 
	@mkdir -p ../lib/gcc_$(__rarinputstream_lib__DIRNAME_SHARED_SUFFIX)

ifeq ($(SHARED),0)
install_rarinputstream_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(RARINPUTSTREAM_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),0)
uninstall_rarinputstream_lib_headers: 
	for f in $(RARINPUTSTREAM_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),1)
../lib/gcc_$(__rarinputstream_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_rarinputstream-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_rarinputstream_dll  $(RARINPUTSTREAM_DLL_OBJECTS)
	$(CXX) $(LINK_DLL_FLAGS) -fPIC -o $@ $(RARINPUTSTREAM_DLL_OBJECTS)  $(VAR) -L../lib $(LDFLAGS)  `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs core,base`
endif

ifeq ($(SHARED),1)
