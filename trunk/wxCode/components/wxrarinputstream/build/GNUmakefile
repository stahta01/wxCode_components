tarball: 
	make distclean
	( cd ../.. && tar -cvzf rarinputstream.tar.gz --exclude="*~" --exclude="*.log" --exclude="*.o*" --exclude="*.a" --exclude=".svn" --exclude="autom4te.cache" rarinputstream/* )

zip: clean
	del ../../rarinputstream.zip
	( cd ../.. && zip -r9 rarinputstream.zip rarinputstream -x "*.pdb" -x "*.log" -x "*.o*" )

