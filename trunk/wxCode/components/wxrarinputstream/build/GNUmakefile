	install -c ../lib/gcc_$(__rarinputstream_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_rarinputstream-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
