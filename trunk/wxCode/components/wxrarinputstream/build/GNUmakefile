	../lib/gcc_$(__rarinputstream_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_rarinputstream-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif
ifeq ($(SHARED),0)
__install_rarinputstream_lib___depname = install_rarinputstream_lib
endif
ifeq ($(SHARED),0)
__uninstall_rarinputstream_lib___depname = uninstall_rarinputstream_lib
endif
ifeq ($(SHARED),0)
__rarinputstream_lib__DIRNAME_SHARED_SUFFIX = lib
endif
ifeq ($(SHARED),1)
__rarinputstream_lib__DIRNAME_SHARED_SUFFIX = dll
endif
