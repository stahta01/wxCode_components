	@%append watcom$(WXLIBPOSTFIX)$(POSTFIX)\wxpdfdoc_dll.lbc system nt_dll
	wlink @watcom$(WXLIBPOSTFIX)$(POSTFIX)\wxpdfdoc_dll.lbc
	wlib -q -n -b ..\lib\wxpdfdoc$(WXLIBPOSTFIX).lib +$^@
!endif

