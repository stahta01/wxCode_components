!ifeq WX_SHARED 0
..\lib\wxpdfdoc$(WXLIBPOSTFIX).lib :  $(WXPDFDOC_LIB_OBJECTS)
	@%create watcom$(WXLIBPOSTFIX)$(POSTFIX)\wxpdfdoc_lib.lbc
	@for %i in ($(WXPDFDOC_LIB_OBJECTS)) do @%append watcom$(WXLIBPOSTFIX)$(POSTFIX)\wxpdfdoc_lib.lbc +%i
	wlib -q -p4096 -n -b $^@ @watcom$(WXLIBPOSTFIX)$(POSTFIX)\wxpdfdoc_lib.lbc
!endif

