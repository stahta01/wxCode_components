..\lib\wxpdfdoc$(WXLIBPOSTFIX).dll :  $(WXPDFDOC_DLL_OBJECTS)
	@%create watcom$(WXLIBPOSTFIX)$(POSTFIX)\wxpdfdoc_dll.lbc
	@%append watcom$(WXLIBPOSTFIX)$(POSTFIX)\wxpdfdoc_dll.lbc option quiet
	@%append watcom$(WXLIBPOSTFIX)$(POSTFIX)\wxpdfdoc_dll.lbc name $^@
