	rm -f gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)/*.d
	rm -f ../lib/gcc_$(__pdfdoc_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_pdfdoc-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
	rm -f ../lib/$(COMPILER_PREFIX)_$(__pdfdoc_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_pdfdoc-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f ../lib/$(COMPILER_PREFIX)_$(__pdfdoc_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_pdfdoc-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f ../samples/minimal
	rm -f ../makefont/makefont

test_for_selected_wxbuild: 
	@$(WX_CONFIG) $(WX_CONFIG_FLAGS)

ifeq ($(WX_SHARED),0)
../lib/gcc_$(__pdfdoc_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_pdfdoc-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_pdfdoc_lib  $(PDFDOC_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(PDFDOC_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(WX_SHARED),0)
install_pdfdoc_lib: $(__pdfdoc_lib___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__pdfdoc_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_pdfdoc-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
endif

ifeq ($(WX_SHARED),0)
uninstall_pdfdoc_lib: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_pdfdoc-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_pdfdoc_lib: 
	@mkdir -p ../lib/gcc_$(__pdfdoc_lib__DIRNAME_SHARED_SUFFIX)

ifeq ($(WX_SHARED),0)
install_pdfdoc_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(PDFDOC_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),0)
uninstall_pdfdoc_lib_headers: 
	for f in $(PDFDOC_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),1)
../lib/$(COMPILER_PREFIX)_$(__pdfdoc_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_pdfdoc-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_pdfdoc_dll  $(PDFDOC_DLL_OBJECTS)
	$(CXX) $(LINK_DLL_FLAGS) -fPIC -o $@ $(PDFDOC_DLL_OBJECTS)  $(VAR_3) -L../lib $(LDFLAGS)  `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs xml,core,base`
endif

ifeq ($(WX_SHARED),1)
install_pdfdoc_dll: $(__pdfdoc_dll___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/$(COMPILER_PREFIX)_$(__pdfdoc_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_pdfdoc-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
	install -c ../lib/$(COMPILER_PREFIX)_$(__pdfdoc_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_pdfdoc-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
endif

ifeq ($(WX_SHARED),1)
uninstall_pdfdoc_dll: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_pdfdoc-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_pdfdoc-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
endif

make_dir_pdfdoc_dll: 
	@mkdir -p ../lib/$(COMPILER_PREFIX)_$(__pdfdoc_dll__DIRNAME_SHARED_SUFFIX)

ifeq ($(WX_SHARED),1)
install_pdfdoc_dll_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(PDFDOC_DLL_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),1)
uninstall_pdfdoc_dll_headers: 
	for f in $(PDFDOC_DLL_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

