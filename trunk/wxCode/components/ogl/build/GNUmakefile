	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_ogl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_ogl_lib: 
	@mkdir -p ../lib/gcc_$(__ogl)

ifeq ($(WX_SHARED),0)
install_ogl_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in  ; do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),0)
uninstall_ogl_lib_headers: 
	for f in  ; do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),1)
../lib/gcc_$(__ogl)/libwxcode_$(WX_PORT)_ogl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_ogl_dll  $(OGL_DLL_OBJECTS)
	$(CXX) -shared -fPIC -o $@ $(OGL_DLL_OBJECTS) $(LDFLAGS) $(____ogl_5) -L../lib  `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs core,base`
endif

ifeq ($(WX_SHARED),1)
install_ogl_dll: 
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__ogl)/libwxcode_$(WX_PORT)_ogl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
	install -c ../lib/gcc_$(__ogl)/libwxcode_$(WX_PORT)_ogl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
endif

