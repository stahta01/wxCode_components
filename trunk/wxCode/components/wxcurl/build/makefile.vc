	-if exist ..\samples\protocols\protocols.exe del ..\samples\protocols\protocols.exe
	-if exist ..\samples\protocols\protocols.ilk del ..\samples\protocols\protocols.ilk
	-if exist ..\samples\protocols\protocols.pdb del ..\samples\protocols\protocols.pdb
