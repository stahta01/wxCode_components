	cd ..\thirdparty\curl\build
	$(MAKE) -f makefile.vc $(MAKEARGS) clean
	cd "$(MAKEDIR)"
	-if exist ..\lib\vc_$(____wxcurl_7)\wxcode_msw$(WX_VERSION)$(WXLIBPOSTFIX)_wxcurl.lib del ..\lib\vc_$(____wxcurl_7)\wxcode_msw$(WX_VERSION)$(WXLIBPOSTFIX)_wxcurl.lib
