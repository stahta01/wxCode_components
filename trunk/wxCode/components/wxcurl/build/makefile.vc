clean: 
	-if exist vcmsw$(WXLIBPOSTFIX)\*.obj del vcmsw$(WXLIBPOSTFIX)\*.obj
	-if exist vcmsw$(WXLIBPOSTFIX)\*.res del vcmsw$(WXLIBPOSTFIX)\*.res
	-if exist vcmsw$(WXLIBPOSTFIX)\*.pch del vcmsw$(WXLIBPOSTFIX)\*.pch
	cd ..\thirdparty\curl\build
	$(MAKE) -f makefile.vc $(MAKEARGS) clean
	cd "$(MAKEDIR)"
	-if exist ..\lib\vc_lib\wxcode_msw$(WX_VERSION)$(WXLIBPOSTFIX)_wxcurl.lib del ..\lib\vc_lib\wxcode_msw$(WX_VERSION)$(WXLIBPOSTFIX)_wxcurl.lib
