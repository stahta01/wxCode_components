ifeq ($(WX_SHARED),0)
../lib/gcc_$(__wxcurl)/libwxcode_$(WX_PORT)_wxcurl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_wxcurl_lib  $(WXCURL_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(WXCURL_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(WX_SHARED),0)
install_wxcurl_lib: 
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__wxcurl)/libwxcode_$(WX_PORT)_wxcurl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
endif

ifeq ($(WX_SHARED),0)
uninstall_wxcurl_lib: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_wxcurl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_wxcurl_lib: 
	@mkdir -p ../lib/gcc_$(__wxcurl)

ifeq ($(WX_SHARED),0)
install_wxcurl_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(WXCURL_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),0)
uninstall_wxcurl_lib_headers: 
	for f in $(WXCURL_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),1)
../lib/gcc_$(__wxcurl)/libwxcode_$(WX_PORT)_wxcurl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_wxcurl_dll  $(WXCURL_DLL_OBJECTS)
	$(CXX) -shared -fPIC -o $@ $(WXCURL_DLL_OBJECTS) $(LDFLAGS) $(VAR_3) -L../lib `curl-config --libs`  -lcurl `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs xml,core,base`
endif

ifeq ($(WX_SHARED),1)
install_wxcurl_dll: 
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__wxcurl)/libwxcode_$(WX_PORT)_wxcurl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
	install -c ../lib/gcc_$(__wxcurl)/libwxcode_$(WX_PORT)_wxcurl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
endif

ifeq ($(WX_SHARED),1)
