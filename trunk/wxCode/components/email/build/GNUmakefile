all: gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)
gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX):
	@mkdir -p gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)

### Targets: ###

all: test_for_selected_wxbuild $(__wxsmtp_lib___depname) $(__wxsmtp_dll___depname) ../samples/SendMail/SendMail

install: $(__install_wxsmtp_lib___depname) $(__install_wxsmtp_lib_headers___depname) $(__install_wxsmtp_dll___depname) $(__install_wxsmtp_dll_headers___depname)

uninstall: $(__uninstall_wxsmtp_lib___depname) $(__uninstall_wxsmtp_lib_headers___depname) $(__uninstall_wxsmtp_dll___depname) $(__uninstall_wxsmtp_dll_headers___depname)

clean: 
	rm -f gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)/*.o
	rm -f gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)/*.d
	rm -f ../lib/gcc_$(__wxsmtp_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
	rm -f ../lib/gcc_$(__wxsmtp_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f ../lib/gcc_$(__wxsmtp_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f ../samples/SendMail/SendMail

test_for_selected_wxbuild: 
	@$(WX_CONFIG) $(WX_CONFIG_FLAGS)

ifeq ($(SHARED),0)
../lib/gcc_$(__wxsmtp_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_wxsmtp_lib  $(WXSMTP_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(WXSMTP_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(SHARED),0)
install_wxsmtp_lib: $(__wxsmtp_lib___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__wxsmtp_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
endif

ifeq ($(SHARED),0)
uninstall_wxsmtp_lib: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_wxsmtp_lib: 
	@mkdir -p ../lib/gcc_$(__wxsmtp_lib__DIRNAME_SHARED_SUFFIX)

ifeq ($(SHARED),0)
install_wxsmtp_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(WXSMTP_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),0)
uninstall_wxsmtp_lib_headers: 
	for f in $(WXSMTP_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),1)
../lib/gcc_$(__wxsmtp_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_wxsmtp_dll  $(WXSMTP_DLL_OBJECTS)
	$(CXX) $(LINK_DLL_FLAGS) -fPIC -o $@ $(WXSMTP_DLL_OBJECTS)  $(VAR_1) -L../lib $(LDFLAGS)  `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs net,core,base`
endif

ifeq ($(SHARED),1)
install_wxsmtp_dll: $(__wxsmtp_dll___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__wxsmtp_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
	install -c ../lib/gcc_$(__wxsmtp_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
endif

ifeq ($(SHARED),1)
uninstall_wxsmtp_dll: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
endif

make_dir_wxsmtp_dll: 
	@mkdir -p ../lib/gcc_$(__wxsmtp_dll__DIRNAME_SHARED_SUFFIX)

ifeq ($(SHARED),1)
install_wxsmtp_dll_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(WXSMTP_DLL_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),1)
uninstall_wxsmtp_dll_headers: 
	for f in $(WXSMTP_DLL_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

../samples/SendMail/SendMail: $(SENDMAIL_OBJECTS) make_sample_dir_SendMail $(__wxsmtp_lib___depname)
	$(CXX) -o $@ $(SENDMAIL_OBJECTS)  $(VAR_1) -L../lib  $(__SendMail_RPATH) $(LDFLAGS)  ../lib/gcc_$(__wxsmtp_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsmtp-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs net,core,base`

make_sample_dir_SendMail: 
	@mkdir -p ../samples/SendMail

