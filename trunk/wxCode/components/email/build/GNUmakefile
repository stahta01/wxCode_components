all: gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)
gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX):
	@mkdir -p gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)

### Targets: ###

all: test_for_selected_wxbuild $(__wxEMail_lib___depname) $(__wxEMail_dll___depname) ../samples/SendMail/wxEMailSmtp ../samples/Pop3Client/wxEMailPop3

install: $(__install_wxEMail_lib___depname) $(__install_wxEMail_lib_headers___depname) $(__install_wxEMail_dll___depname) $(__install_wxEMail_dll_headers___depname)

uninstall: $(__uninstall_wxEMail_lib___depname) $(__uninstall_wxEMail_lib_headers___depname) $(__uninstall_wxEMail_dll___depname) $(__uninstall_wxEMail_dll_headers___depname)

clean: 
	rm -f gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)/*.o
	rm -f gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)/*.d
	rm -f ../lib/gcc_$(__wxEMail_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
	rm -f ../lib/gcc_$(__wxEMail_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f ../lib/gcc_$(__wxEMail_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f ../samples/SendMail/wxEMailSmtp
	rm -f ../samples/Pop3Client/wxEMailPop3

test_for_selected_wxbuild: 
	@$(WX_CONFIG) $(WX_CONFIG_FLAGS)

ifeq ($(SHARED),0)
../lib/gcc_$(__wxEMail_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_wxEMail_lib  $(WXEMAIL_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(WXEMAIL_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(SHARED),0)
install_wxEMail_lib: $(__wxEMail_lib___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__wxEMail_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
endif

ifeq ($(SHARED),0)
uninstall_wxEMail_lib: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_wxEMail_lib: 
	@mkdir -p ../lib/gcc_$(__wxEMail_lib__DIRNAME_SHARED_SUFFIX)

ifeq ($(SHARED),0)
install_wxEMail_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(WXEMAIL_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),0)
uninstall_wxEMail_lib_headers: 
	for f in $(WXEMAIL_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),1)
../lib/gcc_$(__wxEMail_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_wxEMail_dll  $(WXEMAIL_DLL_OBJECTS)
	$(CXX) $(LINK_DLL_FLAGS) -fPIC -o $@ $(WXEMAIL_DLL_OBJECTS)  $(____wxEMail_5) -L../lib $(LDFLAGS)  `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs net,core,base`
endif

ifeq ($(SHARED),1)
install_wxEMail_dll: $(__wxEMail_dll___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__wxEMail_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
	install -c ../lib/gcc_$(__wxEMail_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
endif

ifeq ($(SHARED),1)
uninstall_wxEMail_dll: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
endif

make_dir_wxEMail_dll: 
	@mkdir -p ../lib/gcc_$(__wxEMail_dll__DIRNAME_SHARED_SUFFIX)

ifeq ($(SHARED),1)
install_wxEMail_dll_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(WXEMAIL_DLL_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),1)
uninstall_wxEMail_dll_headers: 
	for f in $(WXEMAIL_DLL_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

../samples/SendMail/wxEMailSmtp: $(WXEMAILSMTP_OBJECTS) make_sample_dir_wxEMailSmtp $(__wxEMail_lib___depname)
	$(CXX) -o $@ $(WXEMAILSMTP_OBJECTS)  $(____wxEMail_5) -L../lib  $(__wxEMail) $(LDFLAGS)  ../lib/gcc_$(__wxEMail_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs net,core,base`

make_sample_dir_wxEMailSmtp: 
	@mkdir -p ../samples/SendMail

../samples/Pop3Client/wxEMailPop3: $(WXEMAILPOP3_OBJECTS) make_sample_dir_wxEMailPop3 $(__wxEMail_lib___depname)
	$(CXX) -o $@ $(WXEMAILPOP3_OBJECTS)  $(____wxEMail_5) -L../lib  $(__wxEMail) $(LDFLAGS)  ../lib/gcc_$(__wxEMail_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_email-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs html,net,core,base`

make_sample_dir_wxEMailPop3: 
	@mkdir -p ../samples/Pop3Client

