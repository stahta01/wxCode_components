	$(INSTALL_DIR) $(DESTDIR)$(datadir)/locale
	for l in $(LOCALE_LINGUAS) ; do \
	$(INSTALL_DIR) $(DESTDIR)$(datadir)/locale/$$l ; \
	$(INSTALL_DIR) $(DESTDIR)$(datadir)/locale/$$l/LC_MESSAGES ; \
	if test -f ./locale/$$l.mo ; then \
	$(INSTALL_DATA) ./locale/$$l.mo $(DESTDIR)$(datadir)/locale/$$l/LC_MESSAGES/webupdater.mo ; \
	fi ; \
	done

xrcscan: 
	wxrc ./src/webupdatedlg.xrc -g -c -o ./src/webupdatedlg.xrc.extracted

webupdater.po: xrcscan
	touch locale/it/webupdater.po
	find ./include -name "*.h" | xargs xgettext --c++ -k_ -kwxPLURAL:1,2 -kwxTRANSLATE -s -j -o locale/it/webupdater.po
	find ./src -name "*.cpp" | xargs xgettext --c++ -k_ -kwxPLURAL:1,2 -kwxTRANSLATE -s -j -o locale/it/webupdater.po
	find ./src -name "*.xrc.*" | xargs xgettext --c++ -k_ -kwxPLURAL:1,2 -kwxTRANSLATE -s -j -o locale/it/webupdater.po
	touch locale/de/webupdater.po
	find ./include -name "*.h" | xargs xgettext --c++ -k_ -kwxPLURAL:1,2 -kwxTRANSLATE -s -j -o locale/de/webupdater.po
	find ./src -name "*.cpp" | xargs xgettext --c++ -k_ -kwxPLURAL:1,2 -kwxTRANSLATE -s -j -o locale/de/webupdater.po
	find ./src -name "*.xrc.*" | xargs xgettext --c++ -k_ -kwxPLURAL:1,2 -kwxTRANSLATE -s -j -o locale/de/webupdater.po
	msgfmt --statistics -o locale/it/webupdater.mo locale/it/webupdater.po
	msgfmt --statistics -o locale/de/webupdater.mo locale/de/webupdater.po

