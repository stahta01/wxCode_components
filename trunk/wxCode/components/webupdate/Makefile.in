locale_install: 
	$(INSTALL_DIR) $(DESTDIR)$(datadir)/locale
	for l in $(LOCALE_LINGUAS) ; do \
	$(INSTALL_DIR) $(DESTDIR)$(datadir)/locale/$$l ; \
	$(INSTALL_DIR) $(DESTDIR)$(datadir)/locale/$$l/LC_MESSAGES ; \
	if test -f $(srcdir)/locale/$$l.mo ; then \
	$(INSTALL_DATA) $(srcdir)/locale/$$l.mo $(DESTDIR)$(datadir)/locale/$$l/LC_MESSAGES/webupdater.mo ; \
	fi ; \
	done

locale_uninstall: 
	for l in $(LOCALE_LINGUAS) ; do \
	if test -f $(DESTDIR)$(datadir)/locale/$$l/LC_MESSAGES/webupdater.mo ; then \
	rm -f $(DESTDIR)$(datadir)/locale/$$l/LC_MESSAGES/webupdater.mo ; \
	fi ; \
	done

i18n: 
	wxrc $(srcdir)/src/webupdatedlg.xrc -g -c -o $(srcdir)/src/webupdatedlg.xrc.extracted
	rm -f locale/webupdater.pot
	touch locale/webupdater.pot
	find $(srcdir)/include -name "*.h" | xargs xgettext --c++ -k_ -kwxPLURAL:1,2 -kwxTRANSLATE -s -j -o locale/webupdater.pot
	find $(srcdir)/src -name "*.cpp" | xargs xgettext --c++ -k_ -kwxPLURAL:1,2 -kwxTRANSLATE -s -j -o locale/webupdater.pot
	find $(srcdir)/src -name "*.xrc.extracted" | xargs xgettext --c++ -k_ -kwxPLURAL:1,2 -kwxTRANSLATE -s -j -o locale/webupdater.pot
	msgmerge locale/it.po locale/webupdater.pot > locale/it.po.new && mv locale/it.po.new locale/it.po
	msgfmt --statistics -o locale/it.mo locale/it.po
	msgmerge locale/de.po locale/webupdater.pot > locale/de.po.new && mv locale/de.po.new locale/de.po
	msgfmt --statistics -o locale/de.mo locale/de.po

