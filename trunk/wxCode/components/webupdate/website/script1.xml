<?xml version="1.0"?>
<!DOCTYPE webupdate SYSTEM "webupdate.dtd">
<!-- $Id$ -->

<webupdate>
   <requires>1.0</requires>
	 
   <!-- for an example of XML webupdate script with multiple packages see script2.xml -->
   <package id="simple">
      <latest-version importance="high">2.0.3</latest-version>
	  
	  <!-- our downloads (1 per platform) -->
      <latest-download>
			<platform>msw</platform>
			<md5>42e898d5f7d5bc1b359c80f02706d01e</md5>
			<uri>http://easynews.dl.sourceforge.net/sourceforge/mathstudio/MathStudio-0.7.2.msi</uri>
	  </latest-download>
      <latest-download>
			<platform>gtk</platform>
			<md5>42e898d5f7d5bc1b359c80f02706d01e</md5>
			<uri>http://wxcode.sourceforge.net/components/webupdate/myapp.tar.gz</uri>
	  </latest-download>

	  <!-- our customized messages -->
      <msg-update-available>The new $(id) $(latest-version) is available for download;
			it has several bugfixes and security improvements !</msg-update-available>
      <msg-update-notavailable>You have the most updated version of $(id)...</msg-update-notavailable>

   </package>
</webupdate>
