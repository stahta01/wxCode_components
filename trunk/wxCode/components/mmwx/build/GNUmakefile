ifeq ($(SHARED),0)
../lib/gcc_$(__mmwx_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_mmwx-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_mmwx_lib  $(MMWX_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(MMWX_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(SHARED),0)
install_mmwx_lib: $(__mmwx_lib___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__mmwx_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_mmwx-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
endif

ifeq ($(SHARED),0)
uninstall_mmwx_lib: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_mmwx-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_mmwx_lib: 
	@mkdir -p ../lib/gcc_$(__mmwx_lib__DIRNAME_SHARED_SUFFIX)

ifeq ($(SHARED),0)
install_mmwx_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(MMWX_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),0)
uninstall_mmwx_lib_headers: 
	for f in $(MMWX_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),1)
../lib/gcc_$(__mmwx_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_mmwx-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_mmwx_dll  $(MMWX_DLL_OBJECTS)
	$(CXX) $(LINK_DLL_FLAGS) -fPIC -o $@ $(MMWX_DLL_OBJECTS)  $(____mmwx_3) -L../lib $(LDFLAGS)  `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs core,base`
endif

ifeq ($(SHARED),1)
install_mmwx_dll: $(__mmwx_dll___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__mmwx_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_mmwx-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
	install -c ../lib/gcc_$(__mmwx_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_mmwx-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
endif

ifeq ($(SHARED),1)
uninstall_mmwx_dll: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_mmwx-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_mmwx-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
endif

make_dir_mmwx_dll: 
	@mkdir -p ../lib/gcc_$(__mmwx_dll__DIRNAME_SHARED_SUFFIX)

ifeq ($(SHARED),1)
install_mmwx_dll_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(MMWX_DLL_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

