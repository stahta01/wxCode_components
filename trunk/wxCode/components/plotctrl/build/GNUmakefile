ifeq ($(WX_SHARED),0)
../lib/gcc_$(__plotctrl)/libwxcode_$(WX_PORT)_plotctrl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_plotctrl_lib  $(PLOTCTRL_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(PLOTCTRL_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(WX_SHARED),0)
install_plotctrl_lib: 
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__plotctrl)/libwxcode_$(WX_PORT)_plotctrl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
endif

ifeq ($(WX_SHARED),0)
uninstall_plotctrl_lib: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_plotctrl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_plotctrl_lib: 
	@mkdir -p ../lib/gcc_$(__plotctrl)

ifeq ($(WX_SHARED),0)
install_plotctrl_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(PLOTCTRL_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),0)
uninstall_plotctrl_lib_headers: 
	for f in $(PLOTCTRL_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),1)
../lib/gcc_$(__plotctrl)/libwxcode_$(WX_PORT)_plotctrl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_plotctrl_dll  $(PLOTCTRL_DLL_OBJECTS)
	$(CXX) -shared -fPIC -o $@ $(PLOTCTRL_DLL_OBJECTS) $(LDFLAGS) $(____plotctrl_3) -L../lib  `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs adv,core,base`
endif

ifeq ($(WX_SHARED),1)
install_plotctrl_dll: 
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__plotctrl)/libwxcode_$(WX_PORT)_plotctrl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
	install -c ../lib/gcc_$(__plotctrl)/libwxcode_$(WX_PORT)_plotctrl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
endif

ifeq ($(WX_SHARED),1)
uninstall_plotctrl_dll: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_plotctrl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_plotctrl-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
endif

