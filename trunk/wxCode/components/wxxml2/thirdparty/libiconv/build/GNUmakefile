..\lib\config.h: ..\lib\config.h.msvc
	cp -pf ..\lib\config.h.msvc ..\lib\config.h

..\include\iconv.h: ..\include\$(CONFIG2_SRCNAME)
	cp -pf ..\include\$(CONFIG2_SRCNAME) ..\include\iconv.h

..\libcharset\include\localcharset.h: ..\libcharset\include\$(CONFIG3_SRCNAME)
	cp -pf ..\libcharset\include\$(CONFIG3_SRCNAME) ..\libcharset\include\localcharset.h

..\libcharset\config.h: ..\libcharset\config.h.msvc
	cp -pf ..\libcharset\config.h.msvc ..\libcharset\config.h

ifeq ($(SHARED),0)
../lib/libiconv.a: $(ICONV_LIB_OBJECTS) ..\lib\config.h ..\include\iconv.h ..\libcharset\include\localcharset.h ..\libcharset\config.h
	rm -f $@
	$(AR) rcu $@ $(ICONV_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(SHARED),1)
../lib/libiconv.so: $(ICONV_DLL_OBJECTS) ..\lib\config.h ..\include\iconv.h ..\libcharset\include\localcharset.h ..\libcharset\config.h
	$(CC) $(LINK_DLL_FLAGS) -fPIC -o $@ $(ICONV_DLL_OBJECTS)  $(__DEBUGINFO) $(LDFLAGS)
endif

