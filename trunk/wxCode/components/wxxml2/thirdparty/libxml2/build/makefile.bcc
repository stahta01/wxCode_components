!if "$(SHARED)" == "0"
..\lib\libxml2.lib: $(LIBXML2_LIB_OBJECTS) ..\config.h ..\lib
	if exist $@ del $@
	tlib /a /p4096 $@ @&&|
	$(LIBXML2_LIB_OBJECTS)
|
!endif

!if "$(SHARED)" == "1"
..\lib\libxml2.dll: $(LIBXML2_DLL_OBJECTS) ..\config.h ..\lib
	ilink32 -Tpd -q  -L$(BCCDIR)\lib -L$(BCCDIR)\lib\psdk  $(__DEBUGINFO) -L$(ICONV_DIR)\lib $(LDFLAGS) @&&|
	c0d32.obj $(LIBXML2_DLL_OBJECTS),$@,, $(__ICONV_LIB_p) $(__WSOCK32_LIB_p) $(__ZLIB_LIB_p) $(__POSIX_LIB_p) import32.lib cw32.lib,,
|
	implib -f ..\lib\libxml2 $@
!endif

