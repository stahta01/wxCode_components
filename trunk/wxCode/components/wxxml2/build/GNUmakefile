	rm -f gcc$(WX_PORT)/*.o
	rm -f gcc$(WX_PORT)/*.d
	rm -f ../lib/gcc_lib/libwxcode_$(WX_PORT)_wxxml2-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
