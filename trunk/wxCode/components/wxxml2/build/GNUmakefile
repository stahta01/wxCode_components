	@$(WX_CONFIG) $(WX_CONFIG_FLAGS)

ifeq ($(WX_SHARED),0)
../lib/gcc_lib/libwxcode_$(WX_PORT)_wxxml2-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_wxxml2_lib  $(WXXML2_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(WXXML2_LIB_OBJECTS)
	$(RANLIB) $@
endif

