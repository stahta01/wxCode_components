make_sample_dir_minimal: 
	@mkdir -p ../sample

tarball: 
	make distclean
	( cd ../.. && tar -cvzf wxxml2.tar.gz --exclude="*~" --exclude="*.log" --exclude="*.o*" --exclude="*.a" --exclude=".svn" --exclude="autom4te.cache" wxxml2/* )

zip: clean
	del ../../wxxml2.zip
	( cd ../.. && zip -r9 wxxml2.zip wxxml2 -x "*.pdb" -x "*.log" -x "*.o*" )

gcc$(WX_PORT)/wxxml2_lib_dtd.o: ../src/dtd.cpp
	$(CXX) -c -o $@ $(WXXML2_LIB_CXXFLAGS) $(CPPDEPS) $<

gcc$(WX_PORT)/wxxml2_lib_xml2.o: ../src/xml2.cpp
	$(CXX) -c -o $@ $(WXXML2_LIB_CXXFLAGS) $(CPPDEPS) $<

gcc$(WX_PORT)/wxxml2_dll_dtd.o: ../src/dtd.cpp
	$(CXX) -c -o $@ $(WXXML2_DLL_CXXFLAGS) $(CPPDEPS) $<

gcc$(WX_PORT)/wxxml2_dll_xml2.o: ../src/xml2.cpp
	$(CXX) -c -o $@ $(WXXML2_DLL_CXXFLAGS) $(CPPDEPS) $<

gcc$(WX_PORT)/minimal_minimal.o: ../sample/minimal.cpp
