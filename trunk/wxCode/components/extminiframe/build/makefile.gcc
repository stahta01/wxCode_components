docs: 
	( cd ..\docs && doxygen )

cleandocs: 
	-if exist ..\docs\html then rmdir /S /Q ..\docs\html

install: all install-wxheaders
	copy /Y ..\lib\libwxextminiframe$(WXLIBPOSTFIX).a $(WX_DIR)$(WXLIBPATH)
	copy /Y ..\lib\wxextminiframe$(WXLIBPOSTFIX).dll $(WX_DIR)$(WXLIBPATH)

install-wxheaders: 
	mkdir $(WX_DIR)\include\wx
	cd .
	copy /Y  ..\include\wx\*.h $(WX_DIR)\include\wx

uninstall: 
	-if exist $(WX_DIR)\include\wx\minibtn.h \
	$(WX_DIR)\include\wx\palettefrm.h \
	$(WX_DIR)\include\wx\palettefrmdef.h then del /Q $(WX_DIR)\include\wx\minibtn.h \
	$(WX_DIR)\include\wx\palettefrm.h \
	$(WX_DIR)\include\wx\palettefrmdef.h
	-if exist $(WX_DIR)$(WXLIBPATH)\*wxextminiframe* then del /Q $(WX_DIR)$(WXLIBPATH)\*wxextminiframe*

ifeq ($(WX_SHARED),0)
..\lib\libwxextminiframe$(WXLIBPOSTFIX).a: $(EXTMINIFRAME_LIB_OBJECTS)
	if exist $@ del $@
	ar rcu $@ $(EXTMINIFRAME_LIB_OBJECTS)
	ranlib $@
endif

ifeq ($(WX_SHARED),1)
..\lib\wxextminiframe$(WXLIBPOSTFIX).dll: $(EXTMINIFRAME_DLL_OBJECTS)
	$(CXX) -shared -fPIC -o $@ $(EXTMINIFRAME_DLL_OBJECTS) $(LDFLAGS) $(__DEBUGINFO) -mthreads -mthreads -L$(WX_DIR)$(WXLIBPATH) -L..\lib -Wl,--out-implib,..\lib\libwxextminiframe$(WXLIBPOSTFIX).a  $(__WXLIB_CORE_NAME_p) $(__WXLIB_BASE_NAME_p) -lwxtiff$(WX3RDPARTYLIBPOSTFIX) -lwxjpeg$(WX3RDPARTYLIBPOSTFIX) -lwxpng$(WX3RDPARTYLIBPOSTFIX) -lwxzlib$(WX3RDPARTYLIBPOSTFIX) -lwxregex$(WXLIBPOSTFIX) -lwxexpat$(WX3RDPARTYLIBPOSTFIX) -lkernel32 -luser32 -lgdi32 -lcomdlg32 -lwinspool -lwinmm -lshell32 -lcomctl32 -lole32 -loleaut32 -luuid -lrpcrt4 -ladvapi32 -lwsock32 -lodbc32
endif

..\sample\minimal.exe: $(MINIMAL_OBJECTS) mingw\minimal_minimal_rc.o $(__extminiframe_lib___depname)
	$(CXX) -o $@ $(MINIMAL_OBJECTS) $(LDFLAGS) $(__DEBUGINFO) -mthreads -mthreads -L$(WX_DIR)$(WXLIBPATH) -L..\lib -Wl,--subsystem,windows -mwindows  ..\lib\libwxextminiframe$(WXLIBPOSTFIX).a $(__WXLIB_CORE_NAME_p) $(__WXLIB_BASE_NAME_p) -lwxtiff$(WX3RDPARTYLIBPOSTFIX) -lwxjpeg$(WX3RDPARTYLIBPOSTFIX) -lwxpng$(WX3RDPARTYLIBPOSTFIX) -lwxzlib$(WX3RDPARTYLIBPOSTFIX) -lwxregex$(WXLIBPOSTFIX) -lwxexpat$(WX3RDPARTYLIBPOSTFIX) -lkernel32 -luser32 -lgdi32 -lcomdlg32 -lwinspool -lwinmm -lshell32 -lcomctl32 -lole32 -loleaut32 -luuid -lrpcrt4 -ladvapi32 -lwsock32 -lodbc32

mingw\extminiframe_lib_minibtncmn.o: ./../src/common/minibtncmn.cpp
	$(CXX) -c -o $@ $(EXTMINIFRAME_LIB_CXXFLAGS) $(CPPDEPS) $<

mingw\extminiframe_lib_palettefrmcmn.o: ./../src/common/palettefrmcmn.cpp
	$(CXX) -c -o $@ $(EXTMINIFRAME_LIB_CXXFLAGS) $(CPPDEPS) $<

mingw\extminiframe_lib_minibtn.o: ./../src/msw/minibtn.cpp
	$(CXX) -c -o $@ $(EXTMINIFRAME_LIB_CXXFLAGS) $(CPPDEPS) $<

mingw\extminiframe_lib_palettefrm.o: ./../src/msw/palettefrm.cpp
	$(CXX) -c -o $@ $(EXTMINIFRAME_LIB_CXXFLAGS) $(CPPDEPS) $<

mingw\extminiframe_dll_minibtncmn.o: ./../src/common/minibtncmn.cpp
	$(CXX) -c -o $@ $(EXTMINIFRAME_DLL_CXXFLAGS) $(CPPDEPS) $<

mingw\extminiframe_dll_palettefrmcmn.o: ./../src/common/palettefrmcmn.cpp
	$(CXX) -c -o $@ $(EXTMINIFRAME_DLL_CXXFLAGS) $(CPPDEPS) $<

mingw\extminiframe_dll_minibtn.o: ./../src/msw/minibtn.cpp
	$(CXX) -c -o $@ $(EXTMINIFRAME_DLL_CXXFLAGS) $(CPPDEPS) $<

mingw\extminiframe_dll_palettefrm.o: ./../src/msw/palettefrm.cpp
	$(CXX) -c -o $@ $(EXTMINIFRAME_DLL_CXXFLAGS) $(CPPDEPS) $<

mingw\minimal_minimal.o: ./../sample/minimal.cpp
	$(CXX) -c -o $@ $(MINIMAL_CXXFLAGS) $(CPPDEPS) $<

mingw\minimal_minimal_rc.o: ./../sample/minimal.rc
	windres --use-temp-file -i$< -o$@ $(__WX_SHAREDDEFINE_p_4) --define HAVE_W32API_H $(__WXUNICODE_DEFINE_p_4) $(__WXDEBUG_DEFINE_p_4) --define __WXMSW__ --include-dir $(WX_DIR)$(WXLIBPATH)\msw$(WXLIBPOSTFIX) --include-dir $(WX_DIR)/include --include-dir ..\include --include-dir ../sample

