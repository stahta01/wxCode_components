clean: 
	-if exist mingw\*.o del mingw\*.o
	-if exist mingw\*.d del mingw\*.d
	-if exist ..\lib\libwxextminiframe$(WXLIBPOSTFIX).a del ..\lib\libwxextminiframe$(WXLIBPOSTFIX).a
	-if exist ..\lib\wxextminiframe$(WXLIBPOSTFIX).dll del ..\lib\wxextminiframe$(WXLIBPOSTFIX).dll
	-if exist ..\lib\libwxextminiframe$(WXLIBPOSTFIX).a del ..\lib\libwxextminiframe$(WXLIBPOSTFIX).a
	-if exist ..\sample\minimal.exe del ..\sample\minimal.exe

tarball: 
	( cd .. && tar -cvzf ..\wxextminiframe.tar.gz --exclude=*.pdb --exclude=*.log --exclude=*.o* * )

