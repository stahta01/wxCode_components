	$(CXX) /c /nologo /TP /Fo$@ $(KEYBINDER_CXXFLAGS) $**

msvc\keybinder_menuutils.obj: .\..\src\menuutils.cpp
	$(CXX) /c /nologo /TP /Fo$@ $(KEYBINDER_CXXFLAGS) $**

msvc\minimal_minimal.obj: .\..\sample\minimal.cpp
	$(CXX) /c /nologo /TP /Fo$@ $(MINIMAL_CXXFLAGS) $**

