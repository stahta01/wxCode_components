tarball: 
	make distclean
	( cd ../.. && tar -cvzf freechart.tar.gz --exclude="*~" --exclude="*.log" --exclude="*.o*" --exclude="*.a" --exclude=".svn" --exclude="autom4te.cache" freechart/* )

zip: clean
	del ../../freechart.zip
	( cd ../.. && zip -r9 freechart.zip freechart -x "*.pdb" -x "*.log" -x "*.o*" )

docs: 
	( cd ../docs && doxygen )

