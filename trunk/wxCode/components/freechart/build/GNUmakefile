ifeq ($(WX_SHARED),0)
../lib/gcc_$(__wxfreechart_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_freechart-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_wxfreechart_lib  $(WXFREECHART_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(WXFREECHART_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(WX_SHARED),0)
install_wxfreechart_lib: 
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__wxfreechart_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_freechart-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
endif

ifeq ($(WX_SHARED),0)
uninstall_wxfreechart_lib: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_freechart-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_wxfreechart_lib: 
	@mkdir -p ../lib/gcc_$(__wxfreechart_lib__DIRNAME_SHARED_SUFFIX)

ifeq ($(WX_SHARED),0)
install_wxfreechart_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(WXFREECHART_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),0)
uninstall_wxfreechart_lib_headers: 
	for f in $(WXFREECHART_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),1)
../lib/gcc_$(__wxfreechart_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_freechart-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_wxfreechart_dll  $(WXFREECHART_DLL_OBJECTS)
	$(CXX) $(LINK_DLL_FLAGS) -fPIC -o $@ $(WXFREECHART_DLL_OBJECTS) $(LDFLAGS) $(____wx_1) -L../lib  `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs gl,core,base`
endif

ifeq ($(WX_SHARED),1)
install_wxfreechart_dll: 
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__wxfreechart_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_freechart-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
	install -c ../lib/gcc_$(__wxfreechart_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_freechart-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
endif

