	@mkdir -p gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)

### Targets: ###

all: test_for_selected_wxbuild $(__wxsf_lib___depname) $(__wxsf_dll___depname) ../samples/wxSFDemo/sfdemo ../samples/wxSFSample1/sfsample1 ../samples/wxSFSample2/sfsample2 ../samples/wxSFSample3/sfsample3 ../samples/wxSFSample4/sfsample4

install: $(__install_wxsf_lib___depname) $(__install_wxsf_lib_headers___depname) $(__install_wxsf_dll___depname) $(__install_wxsf_dll_headers___depname)

uninstall: $(__uninstall_wxsf_lib___depname) $(__uninstall_wxsf_lib_headers___depname) $(__uninstall_wxsf_dll___depname) $(__uninstall_wxsf_dll_headers___depname)

clean: 
	rm -f gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)/*.o
	rm -f gcc$(WX_PORT)$(_BUILDDIR_SHARED_SUFFIX)/*.d
	rm -f ../lib/gcc_$(__wxsf_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
	rm -f ../lib/gcc_$(__wxsf_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f ../lib/gcc_$(__wxsf_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f ../samples/wxSFDemo/sfdemo
	rm -f ../samples/wxSFSample1/sfsample1
	rm -f ../samples/wxSFSample2/sfsample2
	rm -f ../samples/wxSFSample3/sfsample3
	rm -f ../samples/wxSFSample4/sfsample4

test_for_selected_wxbuild: 
	@$(WX_CONFIG) $(WX_CONFIG_FLAGS)

ifeq ($(WX_SHARED),0)
../lib/gcc_$(__wxsf_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_wxsf_lib  $(WXSF_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(WXSF_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(WX_SHARED),0)
install_wxsf_lib: $(__wxsf_lib___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__wxsf_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
endif

ifeq ($(WX_SHARED),0)
uninstall_wxsf_lib: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_wxsf_lib: 
	@mkdir -p ../lib/gcc_$(__wxsf_lib__DIRNAME_SHARED_SUFFIX)

ifeq ($(WX_SHARED),0)
install_wxsf_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in ; do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),0)
uninstall_wxsf_lib_headers: 
	for f in ; do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),1)
../lib/gcc_$(__wxsf_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_wxsf_dll  $(WXSF_DLL_OBJECTS)
	$(CXX) $(LINK_DLL_FLAGS) -fPIC -o $@ $(WXSF_DLL_OBJECTS)  $(VAR_9) -L../lib $(LDFLAGS)  `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs xml,core,base`
endif

ifeq ($(WX_SHARED),1)
install_wxsf_dll: $(__wxsf_dll___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__wxsf_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
	install -c ../lib/gcc_$(__wxsf_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
endif

ifeq ($(WX_SHARED),1)
uninstall_wxsf_dll: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
endif

make_dir_wxsf_dll: 
	@mkdir -p ../lib/gcc_$(__wxsf_dll__DIRNAME_SHARED_SUFFIX)

ifeq ($(WX_SHARED),1)
install_wxsf_dll_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in ; do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),1)
uninstall_wxsf_dll_headers: 
	for f in ; do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

