	$(CXX) -o $@ $(SFDEMO_OBJECTS)  $(VAR_9) -L../lib  $(__sf) $(LDFLAGS)  ../lib/gcc_$(__wxsf_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_wxsf-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs xml,core,base`

