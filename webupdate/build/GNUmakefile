test_for_selected_wxbuild: 
	@$(WX_CONFIG) $(WX_CONFIG_FLAGS)

ifeq ($(WX_SHARED),0)
../lib/gcc_$(__webupdate_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_webupdate-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_webupdate_lib  $(WEBUPDATE_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(WEBUPDATE_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(WX_SHARED),0)
install_webupdate_lib: $(__webupdate_lib___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__webupdate_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_webupdate-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
endif

ifeq ($(WX_SHARED),0)
uninstall_webupdate_lib: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_webupdate-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_webupdate_lib: 
	@mkdir -p ../lib/gcc_$(__webupdate_lib__DIRNAME_SHARED_SUFFIX)

ifeq ($(WX_SHARED),0)
install_webupdate_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(WEBUPDATE_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),0)
uninstall_webupdate_lib_headers: 
	for f in $(WEBUPDATE_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(WX_SHARED),1)
../lib/gcc_$(__webupdate_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_webupdate-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_webupdate_dll  $(WEBUPDATE_DLL_OBJECTS)
	$(CXX) $(LINK_DLL_FLAGS) -fPIC -o $@ $(WEBUPDATE_DLL_OBJECTS)  $(VAR) -L../lib $(LDFLAGS)  $(__CURLDEP_LIB_p) `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs adv,html,adv,xrc,xml,net,core,base`
endif

ifeq ($(WX_SHARED),1)
install_webupdate_dll: $(__webupdate_dll___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__webupdate_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_webupdate-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
	install -c ../lib/gcc_$(__webupdate_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_webupdate-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
endif

ifeq ($(WX_SHARED),1)
uninstall_webupdate_dll: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_webupdate-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_webupdate-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
endif

make_dir_webupdate_dll: 
	@mkdir -p ../lib/gcc_$(__webupdate_dll__DIRNAME_SHARED_SUFFIX)

ifeq ($(WX_SHARED),1)
install_webupdate_dll_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
