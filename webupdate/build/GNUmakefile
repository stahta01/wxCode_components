	$(CXX) -o $@ $(WEBUPDATER_OBJECTS)  $(VAR) -L../lib  $(LDFLAGS)  ../lib/gcc_$(__webupdate_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_webupdate-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(__CURLDEP_LIB_p) `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs adv,html,adv,xrc,xml,net,core,base`

