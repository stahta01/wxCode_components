po_simple100: 
	@mkdir -p ../samples/simple/v1.0.0/lang
	@for f in it.mo de.mo; do \
	if test ! -f ../samples/simple/v1.0.0/lang/$$f -a ! -d ../samples/simple/v1.0.0/lang/$$f ; \
	then x=yep ; \
	else x=`find ../locale/$$f -newer ../samples/simple/v1.0.0/lang/$$f -print` ; \
	fi; \
	case "$$x" in ?*) \
	cp -pRf ../locale/$$f ../samples/simple/v1.0.0/lang ;; \
	esac; \
	done

